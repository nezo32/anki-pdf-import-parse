{
  "name": "anki-pfd-parser",
  "version": "1.0.0",
  "description": "Node.js PDF parser-importer to ANKI cards",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "author": {
    "name": "nezo"
  },
  "license": "MIT",
  "bin": {
    "anki-pfd-parser": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx --watch src/index.ts",
    "start": "node ./dist/index.js",
    "build:start": "npm run build && npm run start",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "prepublish": "npm run build",
    "build:exe": "bun build ./src/index.ts --compile --outfile anki-pfd-parser"
  },
  "dependencies": {
    "cli-progress": "^3.12.0",
    "dotenv": "^17.2.3",
    "openai": "^6.15.0",
    "pdf2json": "^4.0.0",
    "typescript": "^5.9.3",
    "yanki-connect": "^3.2.2"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/node": "^25.0.3",
    "bun": "latest",
    "tsx": "^4.21.0"
  }
}
